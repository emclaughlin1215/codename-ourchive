<p id="notice"><%= notice %></p>

<h1>Listing Collections</h1>

<table>
  <thead>
    <tr>
      <th>Mod</th>
      <th>Date open</th>
      <th>Date closed</th>
      <th>Moderation type</th>
      <th>Challenge description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @collections.each do |collection| %>
      <tr>
        <td><%= collection.mod_id %></td>
        <td><%= collection.date_open %></td>
        <td><%= collection.date_closed %></td>
        <td><%= collection.moderation_type %></td>
        <td><%= collection.challenge_description %></td>
        <td><%= link_to 'Show', collection %></td>
        <td><%= link_to 'Edit', edit_collection_path(collection) %></td>
        <td><%= link_to 'Destroy', collection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Collection', new_collection_path %>
