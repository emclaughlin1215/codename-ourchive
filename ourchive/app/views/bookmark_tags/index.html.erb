<p id="notice"><%= notice %></p>

<h1>Listing Bookmark Tags</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookmark_tags.each do |bookmark_tag| %>
      <tr>
        <td><%= bookmark_tag.user_id %></td>
        <td><%= bookmark_tag.text %></td>
        <td><%= link_to 'Show', bookmark_tag %></td>
        <td><%= link_to 'Edit', edit_bookmark_tag_path(bookmark_tag) %></td>
        <td><%= link_to 'Destroy', bookmark_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bookmark tag', new_bookmark_tag_path %>
