<% cache result do %>
  <dl class="dl-horizontal">
    <dt>Title</dt>
    <dd><%= result.title %></dd>
    <dt>Summary</dt>
    <dd><%= result.work_summary %></dd>
    <dt>Tags</dt>
    <dd><ul class="list-inline">
          <% result.tags.each do |t| %>
            <li><%= t.text %></li>
          <% end %>
        </ul>
    </dd>
  </dl>
<% end %>
